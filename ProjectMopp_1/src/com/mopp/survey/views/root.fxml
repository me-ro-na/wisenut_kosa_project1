<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="rootPane"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.mopp.survey.controllers.SurveyController">
	<children>
		<AnchorPane prefHeight="750.0" prefWidth="800.0">
			<children>
				<Label layoutX="171.0" layoutY="60.0" text="전문가에게 추천받는 영양제"
					textAlignment="CENTER" textFill="BLACK">
					<font>
						<Font name="Malgun Gothic Bold" size="36.0" />
					</font>
				</Label>
				<TableView fx:id="tableView" layoutY="155.0"
					prefHeight="465.0" prefWidth="800.0">
					<columns>
						<TableColumn prefWidth="100" resizable="false"
							text="키" />
						<TableColumn prefWidth="100" resizable="false"
							text="체중" />
						<TableColumn prefWidth="100" resizable="false"
							text="음주" />
						<TableColumn prefWidth="100" resizable="false"
							text="수면" />
						<TableColumn prefWidth="100.0" resizable="false"
							text="고민사항" />
						<TableColumn prefWidth="100.0" resizable="false"
							text="영양제 추천" />
					</columns>
				</TableView>
				<HBox alignment="CENTER" layoutX="320.0" layoutY="658.0"
					prefHeight="50">
					<children>
						<Hyperlink fx:id="btnAdd" text="추가" textFill="#1b1a1a">
							<font>
								<Font size="18.0" />
							</font>
						</Hyperlink>
						<Pane prefHeight="50.0" prefWidth="62.0" />
						<Hyperlink fx:id="btnExit" mnemonicParsing="false"
							prefHeight="35.0" prefWidth="50.0" text="종료" textFill="#1b1a1a">
							<font>
								<Font name="Malgun Gothic Bold" size="18.0" />
							</font>
						</Hyperlink>
					</children>
				</HBox>
			</children>
		</AnchorPane>
	</children>
</StackPane>
