<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Hyperlink?>


<StackPane fx:id="parentContainer"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.mopp.prescription.history.controllers.PrescriptionHistoryController">
	<children>
		<AnchorPane id="AnchorPane" fx:id="container"
			prefHeight="750" prefWidth="800" styleClass="white-bg"
			stylesheets="@../css/Stylesheet.css">
			<children>
				<HBox layoutX="320" layoutY="60">
					<Label fx:id="title" text="" textFill="BLACK"
						textAlignment="CENTER">
						<font>
							<Font name="Malgun Gothic Bold" size="36.0" />
						</font>
					</Label>
				</HBox>
				<Label fx:id="rxID" />


				<TableView fx:id="viewTable" layoutX="98.0" layoutY="320"
					prefHeight="267.0" prefWidth="515.0" AnchorPane.leftAnchor="30.0"
					AnchorPane.rightAnchor="30.0">
					<columns>
						<TableColumn fx:id="colNo" prefWidth="43.0" text="NO" />
						<TableColumn fx:id="colCrDate" prefWidth="104.0"
							text="DATE" />
						<TableColumn fx:id="colMedicine" prefWidth="175.0"
							text="MEDICINE" />
						<TableColumn fx:id="colSym" prefWidth="192.0"
							text="SYMPTOMS" />
						<TableColumn fx:id="colIsTaking" prefWidth="106.0"
							text="TAKING" />
					</columns>
					<placeholder>
						<Label text="No Content">
							<font>
								<Font name="Malgun Gothic Bold" size="20.0"></Font>
							</font>
						</Label>
					</placeholder>
				</TableView>


				<HBox layoutX="286" layoutY="648" fx:id="addButton">
					<Hyperlink onMouseClicked="#open_add_prescription_form"
						textAlignment="CENTER" text="추가하기" textFill="#1b1a1a">
						<font>
							<Font name="Malgun Gothic Bold"  size="18.0" />
						</font>
					</Hyperlink>
				</HBox>
			</children>
		</AnchorPane>
	</children>
</StackPane>
